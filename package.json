{
  "name": "gbf-autopilot",
  "version": "0.0.1",
  "description": "Autopilot is an open source farming/grinding bot for Granblue Fantasy",
  "repository": "github:Frizz925/gbf-autopilot",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production concurrently \"node index.js\" \"python server.py\"",
    "build": "cross-env NODE_ENV=production gulp build",
    "lint": "npm run lint:js && npm run lint:py",
    "lint:js": "eslint index.js server/index.js src/**/*.js",
    "lint:py": "python -m pylint controller test",
    "test": "npm run test:lib && npm run test:server && npm run test:controller",
    "test:lib": "mocha --require babel-register test/lib/**/*.js",
    "test:server": "mocha test/server/**/*.js",
    "test:controller": "python -m nose test.controller",
    "config": "gulp config",
    "key": "python keygen.py"
  },
  "author": "Frizz925 <Frizz925@hotmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-root-import": "^5.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-bluebird": "^1.0.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "concurrently": "^3.4.0",
    "cross-env": "^5.0.1",
    "del": "^3.0.0",
    "eslint": "^4.0.0",
    "eslint-import-resolver-babel-root-import": "0.0.2",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-mocha": "^4.11.0",
    "fancy-log": "^1.3.2",
    "gulp": "^4.0.0",
    "gulp-babel": "^6.1.2",
    "gulp-file-cache": "0.0.1",
    "gulp-livereload": "^3.8.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-shell": "^0.6.3",
    "gulp-sourcemaps": "^2.6.3",
    "mocha": "^4.1.0",
    "plugin-error": "^0.1.2",
    "webpack": "^2.6.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bluebird": "^3.5.1",
    "body-parser": "^1.17.2",
    "dotenv": "^5.0.0",
    "express": "^4.15.3",
    "flat": "^4.0.0",
    "gbf-autopilot-core": "github:Frizz925/gbf-autopilot-core",
    "gbf-autopilot-leech": "github:frizz925/gbf-autopilot-leech",
    "gbf-autopilot-poker": "github:Frizz925/gbf-autopilot-poker",
    "immutable": "^3.8.2",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "rxjs": "^5.5.6",
    "shortid": "^2.2.8",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "winston": "^2.3.1"
  }
}
